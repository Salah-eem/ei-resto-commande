{
  "expo": {
    "name": "RestoCommande - Delivery Driver",
    "slug": "delivery-driver-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "deliverydriverapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "Mobile application for delivery drivers of the RestoCommande restaurant chain, allowing them to manage their deliveries and orders efficiently.",
    "privacy": "public",
    "keywords": [
      "delivery",
      "restaurant",
      "driver",
      "order",
      "food delivery"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eiresto.deliverydriver",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.eiresto.deliverydriver"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8d43e64e-15d2-4f9a-9d2c-e2032dbc5e30"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1c2f8084-dc2f-46fb-8bdd-55241d7035fd"
    },
    "owner": "salah1070"
  }
}
